package numberPrograms;

public class neonOrNot {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int n=9;
		int s=n*n;
		int sum=0;
		while(s>0)
		{
			int r=s%10;
			sum+=r;
			s/=10;
			
		}
		if(sum==n) System.out.println("neon number");
		else System.out.println("not a neon number");
	}

}
