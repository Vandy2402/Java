package numberPrograms;

public class primeOrNot {
	
		public static void main(String[] args)
		{
			int n=454;
			if(n<=1) System.out.println("not a prime");
			else
			{
				boolean flag=true;
			for(int i=2;i<n/2;i++)
			{
				if(n%2==0)
				{
					flag=false;
					break;
				}
			}
			if(flag) System.out.println("prime");
			else System.out.println("not");
			}
		}

}
