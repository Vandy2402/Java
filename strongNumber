package numberPrograms;

public class Strong {
	
		public static void main(String[] args)
		{
			int n=145;
			int num=n;
			int sum=0;
			while(n>0)
			{
				int r=n%10;
				int fact=1;
				for(int i=1;i<=r;i++)
				{
					fact*=i;
				}
				sum+=fact;
				n/=10;
			}
			if(sum==num) System.out.println("strong");
			else System.out.println("not");
		}

}
