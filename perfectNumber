package numberPrograms;

public class perfectNumber {
	
		public static void main(String[] args)
		{
			int a=6;
			int sum=0;
			for(int i=1;i<6;i++)
			{
				if(a%i==0)
					sum=sum+i;
			}
			if(a==sum) System.out.println("Perfect Number");
			else System.out.println("Not a Perfect Number");
		}
}
