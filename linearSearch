package search;

public class linear {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a= {4,3,2,1,42,98};
		System.out.println(search(a,6));

	}
	public static int search(int[] a,int key)
	{
		for(int i=0;i<a.length;i++)
		{
			if(a[i]==key)
				return i;
		}
		return -1;
	}

}
